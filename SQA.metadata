{
	"SQA":
	{
		"window_name": "SQA Objects",
		"flavor": "GlobalObject",
		"actions":
		[
			{
				"name": "setFilePath",
				"description": "Función que permite cambiar la ubicación del archivo de parametros y direcciones.",
				"signature": "/**String*/ nuevaRuta",
				"paramInfo":
				{
					"nuevaRuta": { "description": "Ruta de la nueva ubicación de los archivos" }
				}
			},
			{
				"name": "setNewVariable",
				"description": "Crea una nueva variable en el archivo de parametros.",
				"signature": "/**String*/ variable, /**String*/ valor",
				"paramInfo":
				{
					"variable": { "description": "Nombre de la nueva variable" },
					"valor": { "description": "Valor que contendrá la nueva variable" }
				}
			},
			{
				"name": "getVariable",
				"description": "Obtiene el valor que contiene la variable especificada del archivo de parametros.",
				"signature": "/**String*/ variable",
				"paramInfo":
				{
					"variable": { "description": "Nombre de la variable que se desea obtener" }
				}
			},
				{
				"name": "setRandomOption",
				"description": "Selecciona una al azar una opcion del select.",
				"signature": "/**String*/ variable",
				"paramInfo":
				{
					"variable": { "description": "Nombre de la nueva variable" },
					"valor": { "description": "Valor que contendrá la nueva variable" }
				}
			},
			{
				"name": "editVariable",
				"description": "Modifica el valor de la variable especificada dentro del archivo de parametros.",
				"signature": "/**String*/ variable, /**String*/ valor",
				"paramInfo":
				{
					"variable": { "description": "Nombre de la variable que se desea modificar" },
					"valor": { "description": "Nuevo valor de la variable" }
				}
			},
			{
				"name": "cleanFile",
				"description": "Elimina el contenido del archivo de parametros.",
				"signature": ""
			},
			{
				"name": "queryBD",
				"description": "Hace una consulta a la base de datos dependiendo de los parametros que se envíen.",
				"signature": "/**String*/ consulta, /**Object*/ campos",
				"paramInfo":
				{
					"consulta": { "description": "Query que se desea realizar" },
					"campos": { "description": "Arreglo con los nombres de los campos que se desean obtener" }
				}
			},
			{
				"name": "queryBD2",
				"description": "Hace una consulta a la base de datos y regresa un objeto con las columnas como arreglos",
				"signature": "/**String*/ consulta",
				"paramInfo":
				{
					"consulta": { "description": "Consulta que se desea realizar" }
				}
			},
			{
				"name": "getBdUrl",
				"description": "Obtiene de la base de datos el ambiente especificado.",
				"signature": "/**String*/ idEnv, /**int*/ opQwy",
				"paramInfo":
				{
					"idEnv": { "description": "Id del ambiente en donde se desea probar" },
					"opQwy": { "description": "Indice de la propiedad a obtener (1: URL || 2: CREDENCIAL BD)" }
				}
			},
			{
				"name": "changeSpecialChars",
